apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: traefik
  namespace: kube-system
  annotations:
    kubernetes.io/ingress.class: "traefik"
spec:
  rules:
  - host: traefik
    http:
      paths:
      - path: /
        backend:
          serviceName: traefik
          servicePort: 8080
